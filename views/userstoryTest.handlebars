<div class="container">
    <div class="row">
        <div class="row">
            <div class="col-sm-2 mt-4">
                <a class="btn btn-light" href="/blogtest">Go to Blog</a>
            </div>
        </div>
        <div class="col-md-5 offset-md-3">
            <form id="cms">
                <div class="form-group">
                    <label for="title">Title:</label>
                    <input type="text" class="form-control" id="title">
                    <br />
                    <label for="body">Body:</label>
                    <textarea class="form-control" rows="10" id="body"></textarea>
                    <div class="form-group">
                        <label for="category">Select Category:</label>
                        <select class="custom-select" id="category">
                            <option value="General">General</option>
                            <option value="Scheduling First Dose">Scheduling First Dose</option>
                            <option value="Receiving First Dose">Receiving First Dose</option>
                            <option value="Recovering From First Dose">Recovering From First Dose</option>
                            <option value="Receiving Second Dose">Receiving Second Dose</option>
                            <option value="Recovering From Second Dose">Recovering From Second Dose</option>
                        </select>
                    </div>
                    <br />
                    <button type="submit" class="btn btn-success submit btn-lg">Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>
